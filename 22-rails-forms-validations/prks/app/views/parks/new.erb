<% if !!flash[:errors] %>
  <% flash[:errors].each do |message| %>
    <ul>
      <li><%= message %></li>
    </ul>
  <% end %>
<% end %>

<%= form_for @park do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: "name" %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>
